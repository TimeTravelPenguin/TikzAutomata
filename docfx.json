{
  "metadata": [{
    "src": [{
      "src": "../TikzAutonoma",
      "files": [
        "**.csproj"
      ]
    }],
    "dest": "api",
    "disableGitFeatures": false,
    "disableDefaultFilter": false
  }],
  "build": {
    "content": [{
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [{
      "files": [
        "assets/images/**"
      ]
    }],
    "overwrite": [{
      "files": [
        "apidoc/**.md"
      ],
      "exclude": [
        "bin/**",
        "obj/**",
        "docs/**"
      ]
    }],
    "xref": [
      "y"
    ],
    "dest": "docs",
    "globalMetadata": {
      "_appTitle": "TikzAutonoma",
      "_appFooter": "Developed by Phillip Smith",
      "_appLogoPath": "assets/images/Favicon.svg",
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/TimeTravelPenguin/TikzAutonoma",
        "branch": "dev"
      },
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableNavbar": false,
      "_enableNewTab": true,
      "_enableSearch": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}